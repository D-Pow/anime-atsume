{
  "name": "anime-atsume",
  "version": "0.1.0",
  "description": "A unified website that aggregates and serves anime videos",
  "main": "index.js",
  "author": "D-Pow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D-Pow/anime-atsume.git"
  },
  "config": {
    "buildOutputDir": "../server/src/main/resources/public/"
  },
  "scripts": {
    "start": "webpack serve --config ./config/webpack.config.js",
    "mock": "cross-env MOCK=true npm start",
    "clean": "npx shx rm -rf $npm_package_config_buildOutputDir",
    "clean:node_modules": "npm run clean; mv node_modules znode_modules; ( rm -rf znode_modules & );",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./config/webpack.config.js",
    "test": "jest --coverage tests",
    "create": "node scripts/createComponent.js"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "bootstrap": "^5.3.2",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@svgr/webpack": "^6.5.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "core-js": "^3.32.2",
    "cross-env": "^5.2.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^5.5.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "mock-requests": "^1.3.1",
    "postcss": "^8.4.30",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "shelljs": "^0.8.3",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^9.4.4",
    "typescript": "^3.7.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "eslintConfig": {
    "extends": "./config/.eslintrc.json"
  },
  "jest": {
    "preset": "./config/jest.config.mjs"
  },
  "keywords": [
    "anime",
    "web development",
    "create-react-app",
    "typescript",
    "website",
    "webpack"
  ]
}
